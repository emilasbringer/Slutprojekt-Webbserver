<div class="mainpanel">
    <div class="d-flex justify-content-xl-end" id="mainpanel-closebutton"><div class="btn-close btn-danger"></div></div>
    <h1>Update User</h1>
    <form action="/meeps" method="POST" class="h-75">
        <div class="mt-5 h-75">
            <input class="w-100" type="text" name="name" id="name" placeholder="Name">
            <input class="w-100" type="text" name="phonenumber" id="phonenumber" placeholder="Phone Number">
            <input class="w-100" type="date" name="date" id="date">
        </div>
        <div class="d-flex justify-content-xl-end">
            <button class="btn btn-success" type="submit">Add</button>
        </div>
    </form>
</div>